# .env used for ci
# URLs
URL_FO="{URL_FO}"
URL_BO="{URL_BO}"
URL_INSTALL="{URL_FO}/install-dev/"

# BO account
LOGIN="{EMAIL}"
PASSWD="{PASSWORD}"
FIRSTNAME="{FIRSTNAME}"
LASTNAME="{LASTNAME}"

# Install
INSTALL_LANGUAGE="en"
INSTALL_COUNTRY="France"
DB_SERVER="localhost"
DB_NAME="{DB_NAME}"
DB_USER="root"
DB_PASSWD=""
DB_PREFIX="ps_"
SHOP_NAME="{SHOP_NAME}"

# Browser envs
BROWSER="chromium"
BROWSER_LANG="en-GB"
BROWSER_WIDTH=1680
BROWSER_HEIGHT=900
HEADLESS=true
SLOW_MO=5
INTERCEPT_ERRORS=false

# Screenshots
TAKE_SCREENSHOT_AFTER_FAIL=false

# Mocha
GENERATE_FAILED_STEPS=false

# Maildev envs
SMTP_PORT="1025"
SMTP_SERVER="localhost"
